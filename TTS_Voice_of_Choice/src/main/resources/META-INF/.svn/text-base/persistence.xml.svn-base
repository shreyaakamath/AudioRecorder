<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="TTS_VoiceOfChoice_JPA">
    <provider>
      org.apache.openjpa.persistence.PersistenceProviderImpl
    </provider>
    <class>org.iisc.mile.tts.voiceofchoice.model.UserInfo</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.Sentences</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.KannadaSentences</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.TamilSentences</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.HindiSentences</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.EnglishSentences</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.RecordedAudioId</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.RecordedAudio</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.KannadaRecordedAudio</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.TamilRecordedAudio</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.HindiRecordedAudio</class>
    <class>org.iisc.mile.tts.voiceofchoice.model.EnglishRecordedAudio</class>
    <properties>
      <property name="openjpa.ConnectionURL" value="jdbc:derby:C:/TTS_VoiceOfChoice_Output/TTS-database" />
      <property name="openjpa.ConnectionDriverName" value="org.apache.derby.jdbc.EmbeddedDriver" />
      <!-- <property name="openjpa.Multithreaded" value="true" /> -->
      <property name="openjpa.DynamicEnhancementAgent" value="false" />
    </properties>
  </persistence-unit>
</persistence>
